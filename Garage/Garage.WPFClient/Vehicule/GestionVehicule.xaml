                    <GridViewColumn Header="Nombre de porte" Width="125" DisplayMemberBinding="{Binding Libelle}" />
                    <GridViewColumn Header="libelle" Width="70" DisplayMemberBinding="{Binding Libelle}" />
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Gestion des véhicules" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="198,5,0,0" Height="74" Width="549" FontSize="45"/>
    </Grid>
</Window>
