        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Garage.WPFClient.vehicule"
        mc:Ignorable="d"
        Title="GestionVehicule" Height="615.714" Width="921.429">
    <Grid Margin="0,0,2,4">
        <ListView HorizontalAlignment="Left" Height="487" VerticalAlignment="Top" Width="891" Margin="10,84,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Model" Width="70" DisplayMemberBinding="{Binding Libelle}" />
